        $(' #TxtPassRegMod').on('keyup', function () {
            if ($('#TxtPassRegMod').val() == $('#TxtConPassRegMod').val()) {
                $('#TxtPassRegMod').css('border-color', 'green'),
                $('#TxtConPassRegMod').css('border-color', 'green');
            } else
