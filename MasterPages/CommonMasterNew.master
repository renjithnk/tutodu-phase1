            } else
                $('#TxtPassRegMod').css('border-color', 'red'),
           $('#TxtConPassRegMod').css('border-color', 'red');
        });
        $(' #TxtEmailRegMod').on('keyup', function () {
            var email = $(' #TxtEmailRegMod').val()
            if (isValidEmailAddress(email)) {
                $("#TxtEmailRegMod").css({
                    "border-color": "green"
                });
            } else {
                $("#TxtEmailRegMod").css({
                    "border-color": "red"
                });
            }
        });
        $('#TxtEmailLogMod').on('keyup', function () {
            var email = $('#TxtEmailLogMod').val()
            if (isValidEmailAddress(email)) {
                $("#TxtEmailLogMod").css({
                    "border-color": "green"
                });
            } else {
                $("#TxtEmailLogMod").css({
                    "border-color": "red"
                });
            }
        });
        function isValidEmailAddress(emailAddress) {
            var pattern = new RegExp(/^(("[\w-\s]+")|([\w-]+(?:\.[\w-]+)*)|("[\w-\s]+")([\w-]+(?:\.[\w-]+)*))(@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$)|(@\[?((25[0-5]\.|2[0-4][0-9]\.|1[0-9]{2}\.|[0-9]{1,2}\.))((25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\.){2}(25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\]?$)/i);
            return pattern.test(emailAddress);
        }
        function TxtNullValidateLog() {
            var email, pass;
            email = $('#TxtEmailLogMod').val();
            pass = $('#TxtPassLogMod').val();
            if (email == '') {
                $("#TxtEmailLogMod").css({
                    "border-color": "red"
                });
                return false;
            }
            else if (!isValidEmailAddress(email)) {
                $("#TxtEmailLogMod").css({
                    "border-color": "red"
                });
                return false;
            }
            else if (pass == '') {
                $("#TxtPassLogMod").css({
                    "border-color": "red"
                });
                return false;
            }

        }
        function TxtNullValidateReg() {
            var email, pass, conpass;
            email = $('#TxtEmailRegMod').val();
            pass = $('#TxtPassRegMod').val();
            conpass = $('#TxtConPassRegMod').val();
            if (email != '') {
                if (isValidEmailAddress(email)) {
                    if (pass != '') {
                        if (conpass != '') {
                            if (conpass == pass) {

                            }
                            else {
                                $("#TxtPassRegMod").css({
                                    "border-color": "red"
                                });
                                $("#TxtConPassRegMod").css({
                                    "border-color": "red"
                                });
                                return false;
                            };
                        }
                        else {
                            $("#TxtConPassRegMod").css({
                                "border-color": "red"
                            });
                            return false;
                        }
                    }
                    else {
                        $("#TxtPassRegMod").css({
                            "border-color": "red"
                        });
                        return false;
                    }
                }
                else {
                    $("#TxtEmailRegMod").css({
                        "border-color": "red"
                    });
                    return false;
                }
            }
            else {
                $("#TxtEmailRegMod").css({
                    "border-color": "red"
                });
                return false;
            }
        }
        function TxtNullValidateSearch() {
            var searchkey;
            searchkey = $('#TxtSearchKey').val();
            if (searchkey == '') {
                //$("#TxtSearchKey").css({
                //    "border": "1px solid red"
                //});
                //$("#LnkBttnSearch").css({
                //    "border": "1px solid red"
                //});
                return false;
            }
        }
        $('#TxtSearchKey').on('keyup', function () {
            var searchkey = $('#TxtSearchKey').val()
